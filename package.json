{
  "name": "wiki-to-json",
  "version": "2.0.0-alpha",
  "description": "A simple tool to convert Wikipedia page content to a valid JSON format",
  "main": "./lib",
  "license": "MIT",
  "authors": [
    "Tomer",
    "DigitalKrony<adam.sivins@gmail.com>"
  ],
  "scripts": {
    "build": "tsc --project ./v1/tsconfig.json && tsc --project ./v2/tsconfig.json",
    "watch:v1": "tsc --project ./v1/tsconfig.json --watch",
    "watch:v2": "tsc --project ./v2/tsconfig.json --watch",
    "start:dev": "npm-run-all --parallel watch:v1 watch:v2"
  },
  "dependencies": {
    "axios": "~1.9.0"
  },
  "devDependencies": {
    "@eslint/js": "~9.25.0",
    "@types/node": "~14.14.6",
    "@types/react": "~19.1.2",
    "@types/react-dom": "~19.1.2",
    "@typescript-eslint/parser": "~8.33.1",
    "@vitejs/plugin-react": "~4.4.1",
    "eslint": "~9.28.0",
    "eslint-plugin-notice": "~1.0.0",
    "eslint-plugin-prettier": "~5.4.1",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-react-refresh": "~0.4.19",
    "globals": "~16.0.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~3.5.3",
    "prettier-eslint": "~16.4.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.30.1",
    "vite": "~6.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DigitalKrony/wiki-to-json.git"
  },
  "keywords": [
    "wikipedia",
    "json",
    "parser",
    "converter",
    "typescript"
  ]
}
